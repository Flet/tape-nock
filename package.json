{
  "name": "tape-nock",
  "description": "Automatically record and playback HTTP calls for each tape test.",
  "version": "1.5.2",
  "author": "Dan Flettre <fletd01@yahoo.com>",
  "bugs": {
    "url": "https://github.com/Flet/tape-nock/issues"
  },
  "devDependencies": {
    "nyc": "^10.2.0",
    "request": "^2.69.0",
    "standard": "*",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0"
  },
  "homepage": "https://github.com/Flet/tape-nock",
  "keywords": [
    "fixtures",
    "http",
    "nock",
    "playback",
    "record",
    "tape",
    "test"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Flet/tape-nock.git"
  },
  "scripts": {
    "test": "npm run test:generic && npm run test:only && standard",
    "test:generic": "tape test/*.js | tap-spec",
    "test:only": "tape test/only/*.js | tap-spec",
    "test:generic:n": "nyc tape test/*.js",
    "test:only:n": "nyc tape test/only/*.js",
    "coverage": "npm run test:generic:n && npm run test:only:n && nyc report --reporter=text-lcov"
  },
  "dependencies": {
    "@types/nock": "^0.54.30",
    "@types/tape": "^4.2.27",
    "nock": "^9.0.9",
    "sanitize-filename": "^1.5.3"
  },
  "contributors": [
    "Arandjel Sarenac <arandjel.sarenac@gmail.com>"
  ],
  "typings": "index.d.ts",
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/**",
        "test/**"
      ]
    }
  }
}
